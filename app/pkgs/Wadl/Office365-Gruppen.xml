  <resources siena:authenticationProviderHref="#PowerAppAuth" base="https://europe-002.azure-apim.net/apim/office365groups">
    <resource path="/{connectionId}/v1.0/groups/{groupId}/members">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <param style="template" name="groupId" type="xs:string" required="true">
        <doc title="Wählen Sie eine Gruppe aus der Dropdownliste aus, oder geben Sie Gruppen-ID ein." />
      </param>
      <method siena:requiresAuthentication="true" name="GET" id="ListGroupMembers" actionName="ListGroupMembers" visibility="advanced" siena:isDeprecated="false">
        <doc title="Gruppenmitglieder auflisten&#xA;Mit diesem Vorgang wird eine Liste aller Mitglieder in der angegebenen Gruppe und deren Details (z. B. Name, Titel, E-Mail usw.) zurückgegeben. Sie können bis zu 1.000 Elemente mithilfe des Parameters „Top“ abfragen. Wenn Sie mehr als 1.000 Werte abrufen möchten, aktivieren Sie Einstellungen-&gt;Paginierungsfunktion, und geben Sie das Schwellenwertlimit an." />
        <request>
          <param style="query" name="$top" type="xs:int">
            <doc title="Anzahl abzurufender Gruppenmitglieder (von 1 bis 999, Standardwert: 100)." />
          </param>
        </request>
        <response>
          <representation mediaType="application/json" element="service:ListGroupMembers_Response" />
        </response>
      </method>
    </resource>
    <resource path="/{connectionId}/v1.0/groups/{groupId}/members/$ref">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <param style="template" name="groupId" type="xs:string" required="true">
