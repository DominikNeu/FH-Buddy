      </param>
      <method siena:requiresAuthentication="true" name="POST" id="AddMemberToGroup" actionName="AddMemberToGroup" visibility="important" siena:isDeprecated="false">
        <doc title="Mitglied zur Gruppe hinzufügen&#xA;Mit diesem Vorgang wird einer O365-Gruppe unter Verwendung eines UPN ein Mitglied hinzugefügt." />
        <request>
          <param style="query" name="userUpn" type="xs:string" required="true">
            <doc title="Der Benutzerprinzipalname (UPN) des Benutzers." />
          </param>
        </request>
      </method>
    </resource>
