  <resources siena:authenticationProviderHref="#PowerAppAuth" base="https://europe-002.azure-apim.net/apim/msnweather">
    <resource path="/{connectionId}/current/{Location}">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <param style="template" name="Location" type="xs:string" required="true">
        <doc title="Die Suchabfrage für den Speicherort. Gültige Eingaben sind Ort, Region, Bundesland, Land, Orientierungspunkt, Postleitzahl, Breiten- und Längengrad" />
      </param>
      <method siena:requiresAuthentication="true" name="GET" id="CurrentWeather" actionName="CurrentWeather" visibility="advanced" siena:isDeprecated="false">
        <doc title="Aktuelles Wetter abrufen&#xA;Hiermit wird das aktuelle Wetter für einen Ort abgerufen." />
        <request>
          <param style="query" name="units" type="xs:string" required="true" default="Imperial">
            <doc title="Das Maßsystem für alle Messwerte in der Anforderung und der Antwort. Gültige Optionen sind &quot;Imperial&quot; und &quot;Metric&quot;." />
            <option value="Imperial" />
            <option value="Metric" />
          </param>
        </request>
        <response>
          <representation mediaType="application/json" element="service:CurrentWeather" />
        </response>
      </method>
    </resource>
    <resource path="/{connectionId}/forecast/today/{Location}">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <param style="template" name="Location" type="xs:string" required="true">
        <doc title="Die Suchabfrage für den Speicherort. Gültige Eingaben sind Ort, Region, Bundesland, Land, Orientierungspunkt, Postleitzahl, Breiten- und Längengrad" />
      </param>
      <method siena:requiresAuthentication="true" name="GET" id="TodaysForecast" actionName="TodaysForecast" visibility="important" siena:isDeprecated="false">
        <doc title="Vorhersage für heute abrufen&#xA;Hiermit wird die heutige Vorhersage für den angegebenen Ort abgerufen." />
        <request>
          <param style="query" name="units" type="xs:string" required="true" default="Imperial">
            <doc title="Das Maßsystem für alle Messwerte in der Anforderung und der Antwort. Gültige Optionen sind &quot;Imperial&quot; und &quot;Metric&quot;." />
            <option value="Imperial" />
            <option value="Metric" />
          </param>
        </request>
        <response>
          <representation mediaType="application/json" element="service:WeatherForecast" />
        </response>
      </method>
    </resource>
    <resource path="/{connectionId}/forecast/tomorrow/{Location}">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <param style="template" name="Location" type="xs:string" required="true">
        <doc title="Die Suchabfrage für den Speicherort. Gültige Eingaben sind Ort, Region, Bundesland, Land, Orientierungspunkt, Postleitzahl, Breiten- und Längengrad" />
      </param>
      <method siena:requiresAuthentication="true" name="GET" id="TomorrowsForecast" actionName="TomorrowsForecast" visibility="advanced" siena:isDeprecated="false">
        <doc title="Vorhersage für morgen abrufen&#xA;Hiermit wird die morgige Vorhersage für den angegebenen Ort abgerufen." />
        <request>
          <param style="query" name="units" type="xs:string" required="true" default="Imperial">
            <doc title="Das Maßsystem für alle Messwerte in der Anforderung und der Antwort. Gültige Optionen sind &quot;Imperial&quot; und &quot;Metric&quot;." />
            <option value="Imperial" />
            <option value="Metric" />
          </param>
        </request>
        <response>
          <representation mediaType="application/json" element="service:WeatherForecast" />
        </response>
      </method>
    </resource>
  </resources>
